          display: inline-block;\r\n            transition: transform 0.3s ease;\r\n            font-size: 20px;\r\n        }\r\n\r\n        .home-button:hover {\r\n          transform: scale(1.2); /* Увеличение на 10% при наведении */\r\n        }\r\n\r\n        /* Эффект при наведении на кнопки */\r\n        .header a:hover {\r\n            background-color: #FFF0F5; /* Светло-серый фон при наведении */\r\n        }\r\n\r\n        /* Контент страницы */\r\n        .content {\r\n            margin-top: 70px; /* Отступ сверху, чтобы контент не загораживался шапкой */\r\n            padding: 20px; /* Внутренние отступы контента */\r\n        }\r\n    </style>\r\n</head>\r\n<body>\r\n    <div class=\"header\">\r\n        <div class=\"btn site-name\"><a id=\"home\" href=\"{% url 'home' %}\" class=\"home-button\">Собирайся!</a></div>\r\n        <div class=\"nav-buttons\">\r\n            <a class=\"btn btn-primary\" href=\"{% url 'suggest' %}\">Предложить встречу</a>\r\n            <a class=\"btn btn-primary\" href=\"{% url 'check' %}\">Чекнуть встречи</a>\r\n        </div>\r\n        <div class=\"auth-buttons\">\r\n            <a class=\"btn btn-primary\" href=\"\">Вход</a>\r\n            <a class=\"btn btn-primary\" href=\"\">Регистрация</a>\r\n        </div>\r\n    </div>\r\n<!-- Блок контента -->\r\n\t<div class=\"content-text\">\r\n{% block content %}\r\n{% endblock %}\r\n\t</div>\r\n<!-- Конец блока контента -->\r\n</body>\r\n</html>
